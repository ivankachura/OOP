#include <iostream> 
#include <string> 
#include <cmath> 

using namespace std; 

class Vektor 
{ 
int x, y; 
public: 
Vektor(){ 
x = 0, y = 0; 
} 

Vektor(int x, int y) 
{ 
this->x = x; 
this->y = y; 
} 

void set_Vektor() 
{ 
int x1, y1, x2, y2;
cout << "Вектор:" << endl; 
cout << "Координаты точек: " << endl; 
cout << "Координаты точек: X1, Y1 >> "; 
cin >> x1 >> y1; 
cout << endl; 
cout << "Координаты точек: X2, Y2 >> "; 
cin >> x2 >> y2; 
cout << endl; 
x = x2 - x1; 
y = y2 - y1; 
} 

double Mod() 
{ 
return sqrt(x*x + y*y); 
} 
}; 

int main() 
{ 
Vektor a; 
a.set_Vektor(); 
cout « a.Mod() « endl;; 
system("pause"); 
